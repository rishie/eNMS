{
  "geographical": {
    "default": "3D",
    "_2D": {
      "latitude": 33,
      "longitude": -96,
      "marker": "Image",
      "tile_layer": "osm",
      "zoom_level": 5,
      "icons": {
        "antenna": [18, 12],
        "firewall": [18, 12],
        "host": [18, 12],
        "optical_switch": [18, 12],
        "regenerator": [18, 12],
        "router": [18, 12],
        "server": [18, 12],
        "switch": [18, 12],
        "site": [22, 22]
      }
    },
    "_3D": {
      "layers": {
        "Natural Earth II": {
          "category": "Low-Resolution Satellite View",
          "icon": "naturalEarthII",
          "tooltip": "Natural Earth II, darkened for contrast.\nhttp://www.naturalearthdata.com/",
          "type": "TileMapServiceImageryProvider",
          "args": { "url": "Assets/Textures/NaturalEarthII" }
        },
        "Open Street Map": {
          "category": "Street View",
          "icon": "openStreetMap",
          "tooltip": "OpenStreetMap (OSM) is a collaborative project to create a free editable map of the world.\nhttp://www.openstreetmap.org",
          "type": "OpenStreetMapImageryProvider",
          "args": { "url": "http://tile.openstreetmap.org/" }
        },
        "Bing Maps Aerial": {
          "category": "High-Resolution Satellite View",
          "icon": "bingAerial",
          "tooltip": "Bing Maps standard road maps, provided by Cesium ion",
          "type": "createWorldImagery",
          "arg": "AERIAL"
        },
        "Bing Maps with Labels": {
          "category": "High-Resolution Satellite View",
          "icon": "bingAerialLabels",
          "tooltip": "Bing Maps aerial imagery with labels, provided by Cesium ion",
          "type": "createWorldImagery",
          "arg": "AERIAL_WITH_LABELS"
        },
        "Bing Maps with Roads": {
          "category": "Street View",
          "icon": "bingRoads",
          "tooltip": "Bing Maps standard road maps, provided by Cesium ion",
          "type": "createWorldImagery",
          "arg": "ROAD"
        },
        "ESRI World Imagery": {
          "category": "High-Resolution Satellite View",
          "icon": "esriWorldImagery",
          "tooltip": "World Imagery provides one meter or better satellite and aerial imagery.\nhttp://www.esri.com",
          "type": "ArcGisMapServerImageryProvider",
          "args": {
            "url": "https://services.arcgisonline.com/ArcGIS/rest/services/World_Imagery/MapServer",
            "enablePickFeatures": false
          }
        },
        "ESRI World Street Map": {
          "category": "Street View",
          "icon": "esriWorldStreetMap",
          "tooltip": "Street-level data for the world.\nhttp://www.esri.com",
          "type": "ArcGisMapServerImageryProvider",
          "args": {
            "url": "https://services.arcgisonline.com/ArcGIS/rest/services/World_Street_Map/MapServer",
            "enablePickFeatures": false
          }
        },
        "ESRI Nat Geographic": {
          "category": "Street View",
          "icon": "esriNationalGeographic",
          "tooltip": "National Geographic World Map service.\nhttp://www.esri.com",
          "type": "ArcGisMapServerImageryProvider",
          "args": {
            "url": "https://services.arcgisonline.com/ArcGIS/rest/services/NatGeo_World_Map/MapServer",
            "enablePickFeatures": false
          }
        },
        "Stamen Watercolor": {
          "category": "Street View",
          "icon": "esriNationalGeographic",
          "tooltip": "Reminiscent of hand drawn maps.\nhttp://maps.stamen.com",
          "type": "OpenStreetMapImageryProvider",
          "args": {
            "url": "https://stamen-tiles.a.ssl.fastly.net/watercolor/",
            "credit": "Map tiles by Stamen Design, under CC BY 3.0. Data by OpenStreetMap, under CC BY SA."
          }
        },
        "Stamen Toner": {
          "category": "Street View",
          "icon": "stamenToner",
          "tooltip": "A high contrast black and white map.\nhttp://maps.stamen.com",
          "type": "OpenStreetMapImageryProvider",
          "args": {
            "url": "https://stamen-tiles.a.ssl.fastly.net/toner/",
            "credit": "Map tiles by Stamen Design, under CC BY 3.0. Data by OpenStreetMap, under CC BY SA."
          }
        },
        "Sentinel-2": {
          "category": "High-Resolution Satellite View",
          "icon": "sentinel-2",
          "tooltip": "Sentinel-2 cloudless by EOX IT Services GmbH (Contains modified Copernicus Sentinel data 2016 and 2017).",
          "type": "IonImageryProvider",
          "args": { "assetId": 3954 }
        },
        "Blue Marble": {
          "category": "Low-Resolution Satellite View",
          "icon": "blueMarble",
          "tooltip": "Blue Marble Next Generation July, 2004 imagery from NASA.",
          "type": "IonImageryProvider",
          "args": { "assetId": 3845 }
        },
        "Earth at night": {
          "category": "Low-Resolution Satellite View",
          "icon": "earthAtNight",
          "tooltip": "The Earth at night, also known as The Black Marble, is a 500 meter resolution global composite imagery layer released by NASA.",
          "type": "IonImageryProvider",
          "args": { "assetId": 3812 }
        }
      }
    }
  },
  "logical": {
    "config": {
      "controlType": "trackball",
      "rendererConfig": { "antialias": true, "alpha": true }
    },
    "display_icons": true,
    "display_link_label": false,
    "display_link_traffic": true,
    "traffic_speed": 0.001,
    "link_width": 2,
    "link_opacity": 0.3
  }
}
